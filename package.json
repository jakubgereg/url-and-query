{
  "name": "url-and-query",
  "version": "1.0.0",
  "description": "Utility functions for handling query parameters in URLs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/**.*"
  ],
  "scripts": {
    "start": "ts-node playground.ts",
    "format": "prettier --ignore-path .gitignore --write '**/*.{ts,tsx,js,html}'",
    "format:check": "prettier --check '**/*.{ts,tsx,js,html}'",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "check": "npm run format:check && npm run typecheck && npm run test",
    "preversion": "npm run check",
    "postversion": "git push && git push --tags && npm publish",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "url-and-query",
    "url",
    "qs",
    "query params",
    "query",
    "parser"
  ],
  "author": {
    "name": "Jakub Gereg"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/lodash": "^4.14.202",
    "@types/qs": "^6.9.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "qs": "^6.11.2"
  }
}
